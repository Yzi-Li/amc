; Must have a white space
;    here v
struct Str =>
	len: i32
	mut siz: i32
	mut s: *i32

fn func(s: *mut Str): i32 =>
	let a: i32 = s.siz
	s.siz = 1
	ret a

pub fn main: i32 =>
	let mut test: Str = { 3, 2, null }
	let test_: *Str = &test
	let a: i32 = test.siz
	test.siz += 1
	test.s = &test.siz
	let b: *i32 = &a
	; let c: i32 = func &test
	let c: i32 = test_.siz
	ret c
