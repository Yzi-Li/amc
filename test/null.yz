fn funca(a: *i32): i32 =>
	let b: i32 = *a
	ret b + 2

fn funcb(a: *i32?): i32 =>
	if a == null =>
		ret 1
	let b: i32 = *a
	ret b

fn funcc(a: *i32?): *i32? =>
	ret a

fn funcd(a: *i32?): *i32? =>
	ret a

fn main: i32 =>
	let a: mut i32 = 1
	let b: *i32? = &a
	let c: mut *i32 = &a
	if b == null =>
		ret 1
	a = [funca b]
	c = [funcd b]
	ret a
