; multiple arguments.
fn func(a: i32, b: i32, c: i32): i32 =>
	ret a

; recursion function call.
fn rec func_rec(i: i32): i32 =>
	ret [func_rec i + 1]

; nest function call.
fn func_(i: i32): i32 =>
	ret [func [func 1, 2, 3], 2, 3]

fn func_no_arg: i32 =>
	ret 0

fn func_no_arg_: i32 =>
	ret [func_no_arg] + 2

fn main: i32 =>
	let a: i32 = [func_no_arg]
	ret [func 1, 2, 3]
